<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
    <name>Most Played</name>
    <!--<name>$LOCALIZE[40096]</name>-->
    <match>all</match>
    <!--<limit>10</limit>-->
    <rule field="playcount" operator="greaterthan">
        <value>0</value>
    </rule>
    <limit>25</limit>
    <order direction="descending">playcount</order>
</smartplaylist>
