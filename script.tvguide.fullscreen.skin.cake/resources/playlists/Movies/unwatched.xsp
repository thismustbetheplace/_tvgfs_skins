<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="movies">
    <name>UnWatched</name>
    <!--<name>$LOCALIZE[40102]</name>-->
    <match>all</match>
    <rule field="playcount" operator="is">
        <value>0</value>
    </rule>
    <limit>20</limit>
    <order direction="descending">sorttitle</order>
</smartplaylist>
