<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="songs">
    <name>Recently Played</name>
    <match>all</match>
    <rule field="lastplayed" operator="inthelast">
        <value>4 weeks</value>
    </rule>
    <limit>50</limit>
    <order direction="descending">lastplayed</order>
</smartplaylist>
